{
  "name": "expo-lib-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for React Native Expo library",
  "main": "dist/module/index.js",
  "react-native": "dist/module/index.js",
  "types": "dist/typescript/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "bob build",
    "tsc": "tsc --noEmit",
    "lint": "eslint src/**/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sk39/expo-lib-boilerplate.git"
  },
  "author": "sk39",
  "license": "MIT",
  "keywords": [
    "react",
    "react native",
    "expo",
    "boilerplate"
  ],
  "devDependencies": {
    "@react-native-community/bob": "^0.4.1",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/react": "^16.9.51",
    "@types/react-native": "^0.57.65",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.22.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "module",
      "typescript"
    ]
  }
}
